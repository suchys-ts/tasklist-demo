{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    [
      "@babel/preset-typescript",
      {
        "allExtensions": true,
        "isTSX": true,
        "allowDeclareFields": true
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-typescript",
      {
        "allExtensions": true,
        "isTSX": true,
        "allowDeclareFields": true
      }
    ],
    "@babel/plugin-proposal-class-properties",
    "@babel/transform-runtime"
  ]
}
